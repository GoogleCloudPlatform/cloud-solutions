{
  "dashboardFilters": [],
  "displayName": "Performance Testing - ${PT_TASK_NAME}",
  "labels": {},
  "mosaicLayout": {
    "columns": 12,
    "tiles": [
      {
        "height": 2,
        "widget": {
          "scorecard": {
            "gaugeView": {
              "lowerBound": 0,
              "upperBound": 50
            },
            "thresholds": [],
            "timeSeriesQuery": {
              "timeSeriesFilter": {
                "aggregation": {
                  "alignmentPeriod": "60s",
                  "crossSeriesReducer": "REDUCE_MAX",
                  "groupByFields": [],
                  "perSeriesAligner": "ALIGN_MAX"
                },
                "filter": "metric.type=\"prometheus.googleapis.com/satools_perftest_pttask_total_workers/gauge\" resource.type=\"prometheus_target\""
              }
            }
          },
          "title": "Total Workers"
        },
        "width": 4,
        "yPos": 4
      },
      {
        "height": 4,
        "widget": {
          "title": "Average Response Time",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "timeSeriesQueryLanguage": "fetch prometheus_target\n| metric\n    'prometheus.googleapis.com/satools_perftest_pttask_avg_response_time/gauge'\n| filter (metric.worker =~ 'locust-worker-.*scenario-1680486164524333340.*')\n| group_by 1m,\n    [value_satools_perftest_pttask_avg_response_time_mean:\n       mean(value.satools_perftest_pttask_avg_response_time)]\n| every 1m\n| group_by [metric.endpoint],\n    [value_satools_perftest_pttask_avg_response_time_mean_mean:\n       mean(value_satools_perftest_pttask_avg_response_time_mean)]"
                }
              }
            ],
            "thresholds": [],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "",
              "scale": "LINEAR"
            }
          }
        },
        "width": 12,
        "yPos": 6
      },
      {
        "height": 2,
        "widget": {
          "scorecard": {
            "gaugeView": {
              "lowerBound": 0,
              "upperBound": 1000
            },
            "thresholds": [],
            "timeSeriesQuery": {
              "timeSeriesFilter": {
                "aggregation": {
                  "alignmentPeriod": "60s",
                  "crossSeriesReducer": "REDUCE_MAX",
                  "groupByFields": [],
                  "perSeriesAligner": "ALIGN_MAX"
                },
                "filter": "metric.type=\"prometheus.googleapis.com/satools_perftest_pttask_total_request_per_sec/gauge\" resource.type=\"prometheus_target\""
              }
            }
          },
          "title": "RPS"
        },
        "width": 4,
        "xPos": 4,
        "yPos": 4
      },
      {
        "height": 4,
        "widget": {
          "title": "Total Users",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "minAlignmentPeriod": "60s",
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_MEAN"
                    },
                    "filter": "metric.type=\"prometheus.googleapis.com/satools_perftest_pttask_total_users/gauge\" resource.type=\"prometheus_target\"",
                    "secondaryAggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_NONE"
                    }
                  }
                }
              }
            ],
            "thresholds": [],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            }
          }
        },
        "width": 6
      },
      {
        "height": 4,
        "widget": {
          "title": "Total Errors",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "satools_perftest_pttask_total_errors"
                }
              }
            ],
            "thresholds": [],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            }
          }
        },
        "width": 6,
        "xPos": 6
      },
      {
        "height": 2,
        "widget": {
          "scorecard": {
            "sparkChartView": {
              "sparkChartType": "SPARK_LINE"
            },
            "thresholds": [],
            "timeSeriesQuery": {
              "timeSeriesFilter": {
                "aggregation": {
                  "alignmentPeriod": "60s",
                  "crossSeriesReducer": "REDUCE_MAX",
                  "groupByFields": [],
                  "perSeriesAligner": "ALIGN_MAX"
                },
                "filter": "metric.type=\"prometheus.googleapis.com/satools_perftest_pttask_total_masters/gauge\" resource.type=\"prometheus_target\""
              }
            }
          },
          "title": "Total Masters"
        },
        "width": 4,
        "xPos": 8,
        "yPos": 4
      }
    ]
  }
}
