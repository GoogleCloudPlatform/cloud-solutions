openapi: 3.0.0
info:
  title: Return Order API
  version: 1.0.0
servers:
  - url: 'https://integrations.googleapis.com'
paths:
  /v2/projects/customermod-genai-sa/locations/us-central1/integrations/apparel-search-prod:execute?triggerId=api_trigger/return-order:
    post:
      summary: update status for returned orders.
      operationId: return_order
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: object
              required:
                - order_id
              properties:
                order_id:
                  type: string
                  description: The unique identifier for the customer's order.
      responses:
        default:
          description: Order data with updated status.
          content:
            application/json:
              schema:
                type: object
                additionalProperties: true
